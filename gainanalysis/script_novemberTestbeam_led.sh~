#!/bin/bash                                                                      
uplinks=(1 2 3 4 5 6 7 8)                                                                                                                      
filename="$1"
while read -r first 
do
    for i in uplinks
    do
	nameLed=$first
	./gainAnalysis --l $nameLed --u ${uplinks[@]} 
    done
    echo "Doing gain anaysis for file:  $nameLed"  
done < "$filename" 

